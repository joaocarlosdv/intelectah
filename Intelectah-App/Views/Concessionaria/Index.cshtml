@model ConcessionariaViewModel;
@{
    ViewData["Title"] = "Concessionária";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="card">
    <div class="card-body">
        <div class="card-header d-flex justify-content-start top-h flex-column align-items-start">
            <h2 class="content-header-title float-left mb-0">@ViewData["Title"]</h2>
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="@Url.Action("Index", "Home")">Home</a></li>
                    <li class="breadcrumb-item active" aria-current="page">@ViewData["Title"]</li>
                </ol>
            </nav>
        </div>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-12">
                <div class="card-body">
                    <div class="table-responsive">

                        <a class="btn btn-primary"
                           href="@Url.Action("Incluir", "Concessionaria")">
                            <i class="bi bi-plus"></i>
                            <span>Novo</span>
                        </a>
                        <br />
                        <br />

                        <table class="table table-striped table-hover"
                               id="myTable">
                        </table>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>

<script src="/views/concessionaria.index.js"></script>